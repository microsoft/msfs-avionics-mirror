<ModelBehaviors>
	<Include ModelBehaviorFile="Asobo\Generic.xml"/>
	<Include ModelBehaviorFile="Asobo\GlassCockpit\AS3000.xml"/>
	<Include ModelBehaviorFile="Asobo\GlassCockpit\Inputs\AS1000_Inputs.xml"/>
	<Include RelativeFile="Common.xml"/>

	<ParametersFn Name="G3000_GTC_Backlight_Var">
		<Parameters Type="Default">
			<RETURN_TYPE_PARAM_NAME>BACKLIGHT_VAR_TYPE</RETURN_TYPE_PARAM_NAME>
			<RETURN_NAME_PARAM_NAME>BACKLIGHT_VAR_NAME</RETURN_NAME_PARAM_NAME>
			<ID>1</ID>
		</Parameters>
		<ReturnParameters>
			<#RETURN_TYPE_PARAM_NAME#>L:1</#RETURN_TYPE_PARAM_NAME#>
			<#RETURN_NAME_PARAM_NAME#>WTG3000_Gtc_Backlight_#ID#</#RETURN_NAME_PARAM_NAME#>
		</ReturnParameters>
	</ParametersFn>

	<Template Name="WT_G3000_GTC_Template">
		<Parameters Type="Default">
			<ID>1</ID>
		</Parameters>

		<UseTemplate Name="ASOBO_GT_Helper_Suffix_ID_Appender">
			<Switch Param="ORIENTATION">
				<Case Value="Horizontal">
					<TEMPLATE_TO_CALL>WT_G3000_GTC_Horizontal_SubTemplate</TEMPLATE_TO_CALL>
				</Case>
				<Default>
					<TEMPLATE_TO_CALL>WT_G3000_GTC_Vertical_SubTemplate</TEMPLATE_TO_CALL>
				</Default>
			</Switch>
		</UseTemplate>
	</Template>

	<!-- Vertical GTC (570/575) -->

	<Template Name="WT_G3000_GTC_Vertical_SubTemplate">
		<Parameters Type="Default">
			<AS3000>AS580</AS3000>
		</Parameters>

		<Parameters Type="Default">
			<SCREEN_NODE_ID>#AS3000#_Screen#SUFFIX_ID#</SCREEN_NODE_ID>
			<INPUT_EVENT_ID_SOURCE>AS3000_TSC_Vertical#SUFFIX_ID#</INPUT_EVENT_ID_SOURCE>
			<KEY_PREFIX>AS3000_TSC_Vertical_#ID#</KEY_PREFIX>
		</Parameters>

		<Parameters Type="Override">
			<UseParametersFn Name="G3000_GTC_Backlight_Var">
			</UseParametersFn>
		</Parameters>

		<Parameters Type="Default">
			<EMISSIVE_MIN>0.02</EMISSIVE_MIN>
			<EMISSIVE_MAX>1</EMISSIVE_MAX>
			<EMISSIVE_GAMMA>2.2</EMISSIVE_GAMMA>
			<EMISSIVE_INPUT>(#BACKLIGHT_VAR_TYPE#:#BACKLIGHT_VAR_NAME#)</EMISSIVE_INPUT>
		</Parameters>

		<Parameters Type="Default">
			<SCREEN_EMISSIVE_MIN>#EMISSIVE_MIN#</SCREEN_EMISSIVE_MIN>
			<SCREEN_EMISSIVE_MAX>#EMISSIVE_MAX#</SCREEN_EMISSIVE_MAX>
			<SCREEN_EMISSIVE_GAMMA>#EMISSIVE_GAMMA#</SCREEN_EMISSIVE_GAMMA>
			<SCREEN_EMISSIVE_INPUT>#EMISSIVE_INPUT#</SCREEN_EMISSIVE_INPUT>
		</Parameters>

		<Parameters Type="Default">
			<KNOB_EMISSIVE_MIN>#EMISSIVE_MIN#</KNOB_EMISSIVE_MIN>
			<KNOB_EMISSIVE_MAX>#EMISSIVE_MAX#</KNOB_EMISSIVE_MAX>
			<KNOB_EMISSIVE_GAMMA>#EMISSIVE_GAMMA#</KNOB_EMISSIVE_GAMMA>
			<KNOB_EMISSIVE_INPUT>#EMISSIVE_INPUT#</KNOB_EMISSIVE_INPUT>
		</Parameters>

		<Parameters Type="Default">
			<UseParametersFn Name="G3000_Emissive_Power_Curve">
				<PREFIX>SCREEN</PREFIX>
			</UseParametersFn>
			<UseParametersFn Name="G3000_Emissive_Power_Curve">
				<PREFIX>KNOB</PREFIX>
			</UseParametersFn>
		</Parameters>

		<UseTemplate Name="ASOBO_AS3000_TSC_Screen_Template">
			<NODE_ID>#SCREEN_NODE_ID#</NODE_ID>
			<AS3000>AS3000</AS3000>
			<EMISSIVE_CODE>#SCREEN_EMISSIVE_CODE#</EMISSIVE_CODE>
		</UseTemplate>

		<Component ID="AS580_Knobs_#ID#">
			<Parameters Type="Default">
				<WWISE_EVENT_LARGE_KNOB>AS580_big_knob_turn</WWISE_EVENT_LARGE_KNOB>
				<WWISE_EVENT_SMALL_KNOB>AS580_knob_turn</WWISE_EVENT_SMALL_KNOB>
				<WWISE_EVENT_PUSH_1>AS580_knob_push_button_on</WWISE_EVENT_PUSH_1>
				<WWISE_EVENT_PUSH_2>AS580_knob_push_button_off</WWISE_EVENT_PUSH_2>
				<COUNT>36</COUNT>
				<NODE_ID_RIGHT_OUTER_KNOB>AS580_KNOB_01#SUFFIX_ID#</NODE_ID_RIGHT_OUTER_KNOB>
				<ANIM_NAME_RIGHT_OUTER_KNOB>AS580_KNOB_01#SUFFIX_ID#</ANIM_NAME_RIGHT_OUTER_KNOB>
				<NODE_ID_RIGHT_INNER_KNOB>AS580_KNOB_01_SMALL#SUFFIX_ID#</NODE_ID_RIGHT_INNER_KNOB>
				<ANIM_NAME_RIGHT_INNER_KNOB>AS580_KNOB_01_SMALL#SUFFIX_ID#</ANIM_NAME_RIGHT_INNER_KNOB>
				<ANIM_NAME_RIGHT_INNER_KNOB_PUSH>AS580_KNOB_01_SMALL_PUSH#SUFFIX_ID#</ANIM_NAME_RIGHT_INNER_KNOB_PUSH>
				<NODE_ID_LEFT_JOYSTICK>AS580_KNOB_02#SUFFIX_ID#</NODE_ID_LEFT_JOYSTICK>
				<PAN_NODE_ID_LEFT_JOYSTICK>AS580_Joystick_Range_Pan#SUFFIX_ID#</PAN_NODE_ID_LEFT_JOYSTICK>
				<ANIM_NAME_LEFT_JOYSTICK_KNOB>AS580_KNOB_02#SUFFIX_ID#</ANIM_NAME_LEFT_JOYSTICK_KNOB>
				<ANIM_NAME_LEFT_JOYSTICK_PUSH>AS580_Joystick_Range_Push#SUFFIX_ID#</ANIM_NAME_LEFT_JOYSTICK_PUSH>
				<ANIM_NAME_LEFT_JOYSTICK_L_R>AS580_Joystick_Range_L_R#SUFFIX_ID#</ANIM_NAME_LEFT_JOYSTICK_L_R>
				<ANIM_NAME_LEFT_JOYSTICK_DN_UP>AS580_Joystick_Range_DN_UP#SUFFIX_ID#</ANIM_NAME_LEFT_JOYSTICK_DN_UP>
				<NODE_ID_MIDDLE_KNOB>AS580_KNOB_03#SUFFIX_ID#</NODE_ID_MIDDLE_KNOB>
				<ANIM_NAME_MIDDLE_KNOB>AS580_KNOB_03#SUFFIX_ID#</ANIM_NAME_MIDDLE_KNOB>
				<ANIM_NAME_MIDDLE_KNOB_PUSH>AS580_KNOB_03_SMALL_PUSH#SUFFIX_ID#</ANIM_NAME_MIDDLE_KNOB_PUSH>
				<LONG_CLICK_TIME>2</LONG_CLICK_TIME>

				<JOYSTICK_THRESHOLD_X_INPUT>0.1</JOYSTICK_THRESHOLD_X_INPUT>
				<JOYSTICK_THRESHOLD_X_MOUSE>0.01</JOYSTICK_THRESHOLD_X_MOUSE>
				<JOYSTICK_THRESHOLD_Y_INPUT>0.1</JOYSTICK_THRESHOLD_Y_INPUT>
				<JOYSTICK_THRESHOLD_Y_MOUSE>0.01</JOYSTICK_THRESHOLD_Y_MOUSE>

				<Condition Valid="NO_KNOB_BACKLIGHT">
					<False>
						<NODE_ID_RIGHT_OUTER_KNOB_BACKLIGHT>AS580_KNOB_01_BACKLIGHT#SUFFIX_ID#</NODE_ID_RIGHT_OUTER_KNOB_BACKLIGHT>
						<NODE_ID_MIDDLE_KNOB_BACKLIGHT>AS580_KNOB_03_BACKLIGHT#SUFFIX_ID#</NODE_ID_MIDDLE_KNOB_BACKLIGHT>
						<NODE_ID_LEFT_JOYSTICK_BACKLIGHT>AS580_KNOB_02_BACKLIGHT#SUFFIX_ID#</NODE_ID_LEFT_JOYSTICK_BACKLIGHT>
					</False>
				</Condition>
			</Parameters>

			<Component ID="#NODE_ID_RIGHT_OUTER_KNOB#" Node="#NODE_ID_RIGHT_OUTER_KNOB#">
				<Parameters Type="Override">
					<KNOB_ID>Right_Outer_Knob</KNOB_ID>
				</Parameters>
				<Parameters Type="Override">
					<TOOLTIP_TITLE>@TT_Package.AS570_RIGHT_OUTER_KNOB_TITLE</TOOLTIP_TITLE>
					<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</TOOLTIP_ENTRY_0>
				</Parameters>
				<UseInputEvent ID="AS3000">
					<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:#KEY_PREFIX#_RightKnob_Large_INC) } els{ (&gt;H:#KEY_PREFIX#_RightKnob_Large_DEC) }</SET_STATE_EXTERNAL>
					<TT_DESCRIPTION_ID>@TT_Package.AS430_KNOB_MHZ_ACTION</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseTemplate Name="ASOBO_GT_Knob_Infinite">
					<ANIM_NAME>#ANIM_NAME_RIGHT_OUTER_KNOB#</ANIM_NAME>
					<PART_ID>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</PART_ID>
					<CLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Inc)</CLOCKWISE_CODE>
					<ANTICLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Dec)</ANTICLOCKWISE_CODE>
					<WWISE_EVENT>#WWISE_EVENT_LARGE_KNOB#</WWISE_EVENT>
					<THRESHOLD_INPUT>0.0005</THRESHOLD_INPUT>
					<THRESHOLD_MOUSE>0.005</THRESHOLD_MOUSE>
				</UseTemplate>
			</Component>

			<Condition NotEmpty="NODE_ID_RIGHT_OUTER_KNOB_BACKLIGHT">
				<Component ID="#NODE_ID_RIGHT_OUTER_KNOB_BACKLIGHT#" Node="#NODE_ID_RIGHT_OUTER_KNOB_BACKLIGHT#">
					<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
						<EMISSIVE_CODE>#KNOB_EMISSIVE_CODE#</EMISSIVE_CODE>
					</UseTemplate>
				</Component>
			</Condition>

			<Component ID="#NODE_ID_RIGHT_INNER_KNOB#" Node="#NODE_ID_RIGHT_INNER_KNOB#">
				<Parameters Type="Override">
					<KNOB_ID>Right_Inner_Knob</KNOB_ID>
					<BTN_ID_EXTERNAL>Right_Inner_Knob_Button</BTN_ID_EXTERNAL>
				</Parameters>
				<Parameters Type="Override">
					<TOOLTIP_TITLE>@TT_Package.AS570_RIGHT_INNER_KNOB_TITLE</TOOLTIP_TITLE>
					<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</TOOLTIP_ENTRY_0>
					<TOOLTIP_ENTRY_1>#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#</TOOLTIP_ENTRY_1>
				</Parameters>
				<UseInputEvent ID="AS3000">
					<TT_INTERACTION>PRIMARY_DOWN+SECONDARY_DOWN</TT_INTERACTION>
					<TT_INTERACTION_LOCKABLE>PRIMARY_DOWN</TT_INTERACTION_LOCKABLE>
					<BTN_ID>#BTN_ID_EXTERNAL#</BTN_ID>
					<!-- Configure event processing -->
					<SET_ARG_COUNT>2</SET_ARG_COUNT><!-- p0: Press/Release, p1:PressDuration, 0 = infinite (if pressing) -->
					<SET_STATE_EXTERNAL>p0 sp0
						l0 0 == if{ (O:_PressTime) 0 &gt; if{ (E:SIMULATION TIME, second) (O:_PressTime) - #LONG_CLICK_TIME# &lt; if{ (&gt;H:#KEY_PREFIX#_RightKnob_Push) 0 (&gt;O:_PressTime) } els{ (&gt;H:#KEY_PREFIX#_RightKnob_Push_Long) 0 (&gt;O:_PressTime) } } }
						l0 1 == if{ (E:SIMULATION TIME, second) (&gt;O:_PressTime) p1 (&gt;O:_PressForDuration) }
					</SET_STATE_EXTERNAL>
					<ANIM>EXTERNAL</ANIM>
					<BINDING_INC_0>Push</BINDING_INC_0>
					<BINDING_INC_0_PARAM_0>1</BINDING_INC_0_PARAM_0>
					<BINDING_DEC_0>Release</BINDING_DEC_0>
					<BINDING_DEC_0_PARAM_0>0</BINDING_DEC_0_PARAM_0>
					<!-- Configure binding for longpress event -->
					<BINDING_SET_0>Push_Long</BINDING_SET_0>
					<BINDING_SET_0_PARAM_0>1</BINDING_SET_0_PARAM_0>
					<BINDING_SET_0_PARAM_1>#LONG_CLICK_TIME#</BINDING_SET_0_PARAM_1>
					<TT_DESCRIPTION_ID>@TT_Package.AS580_UPPER_KNOB_INNER_ACTION_PUSH</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseInputEvent ID="AS3000">
					<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:#KEY_PREFIX#_RightKnob_Small_INC) } els{ (&gt;H:#KEY_PREFIX#_RightKnob_Small_DEC) }</SET_STATE_EXTERNAL>
					<TT_DESCRIPTION_ID>@TT_Package.AS430_KNOB_MHZ_ACTION</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseTemplate Name="ASOBO_GT_Knob_Infinite_Push_Timed">
					<CURSOR_LEFT>TurnLeftSmall</CURSOR_LEFT>
					<CURSOR_RIGHT>TurnRightSmall</CURSOR_RIGHT>
					<ANIM_NAME_KNOB>#ANIM_NAME_RIGHT_INNER_KNOB#</ANIM_NAME_KNOB>
					<ANIM_NAME_PUSH>#ANIM_NAME_RIGHT_INNER_KNOB_PUSH#</ANIM_NAME_PUSH>
					<PART_ID>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</PART_ID>
					<CLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Inc)</CLOCKWISE_CODE>
					<ANTICLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Dec)</ANTICLOCKWISE_CODE>
					<LEFT_SINGLE_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#_Push)</LEFT_SINGLE_CODE>
					<LEFT_LEAVE_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#_Release)</LEFT_LEAVE_CODE>
					<TIMER_EVENTS_HANDLED_EXTERNALLY>True</TIMER_EVENTS_HANDLED_EXTERNALLY>
					<LOCKABLE_EVENT_SECONDARY>LeftRelease</LOCKABLE_EVENT_SECONDARY>
					<NO_LOCK_EVENT_SECONDARY>RightRelease</NO_LOCK_EVENT_SECONDARY>
					<MOUSEFLAGS_DRAG_IM>RightAll+LeftAll+Wheel+Lock+Unlock</MOUSEFLAGS_DRAG_IM>
					<WWISE_EVENT>#WWISE_EVENT_SMALL_KNOB#</WWISE_EVENT>
					<WWISE_EVENT_1>#WWISE_EVENT_PUSH_1#</WWISE_EVENT_1>
					<WWISE_EVENT_2>#WWISE_EVENT_PUSH_2#</WWISE_EVENT_2>
					<THRESHOLD_X_INPUT>0.05</THRESHOLD_X_INPUT>
					<THRESHOLD_X_MOUSE>0.001</THRESHOLD_X_MOUSE>
				</UseTemplate>
			</Component>
			<UseTemplate Name="ASOBO_GT_Interaction_Tooltip">
				<NODE_ID>#NODE_ID_RIGHT_OUTER_KNOB#</NODE_ID>
			</UseTemplate>

			<Component ID="#NODE_ID_MIDDLE_KNOB#" Node="#NODE_ID_MIDDLE_KNOB#">
				<Parameters Type="Override">
					<KNOB_ID>Middle_Knob</KNOB_ID>
					<BTN_ID_EXTERNAL>Middle_Knob_Button</BTN_ID_EXTERNAL>
				</Parameters>
				<Parameters Type="Override">
					<TOOLTIP_TITLE>@TT_Package.AS570_MIDDLE_KNOB_TITLE</TOOLTIP_TITLE>
					<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</TOOLTIP_ENTRY_0>
				</Parameters>
				<UseInputEvent ID="AS3000">
					<BTN_ID>#BTN_ID_EXTERNAL#</BTN_ID>
					<ANIM>HOLD</ANIM>
					<ANIM_VAR>_PushAnimVar</ANIM_VAR>
					<TT_INTERACTION>PRIMARY_DOWN+SECONDARY_DOWN</TT_INTERACTION>
					<TT_INTERACTION_LOCKABLE>PRIMARY_DOWN</TT_INTERACTION_LOCKABLE>
					<BINDING_INC_0>Push</BINDING_INC_0>
					<BINDING_INC_0_PARAM_0>1</BINDING_INC_0_PARAM_0>
					<SET_STATE_EXTERNAL>p0 if{ (&gt;H:#KEY_PREFIX#_MiddleKnob_Push) }</SET_STATE_EXTERNAL>
				</UseInputEvent>
				<UseInputEvent ID="AS3000">
					<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:#KEY_PREFIX#_MiddleKnob_INC) } els{ (&gt;H:#KEY_PREFIX#_MiddleKnob_DEC) }</SET_STATE_EXTERNAL>
					<TT_DESCRIPTION_ID>@TT_Package.AS570_MIDDLE_KNOB_ACTION</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseTemplate Name="ASOBO_GT_Knob_Infinite_Push">
					<ANIM_NAME_KNOB>#ANIM_NAME_MIDDLE_KNOB#</ANIM_NAME_KNOB>
					<ANIM_NAME_PUSH>#ANIM_NAME_MIDDLE_KNOB_PUSH#</ANIM_NAME_PUSH>
					<PART_ID>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</PART_ID>
					<CLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Inc)</CLOCKWISE_CODE>
					<ANTICLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Dec)</ANTICLOCKWISE_CODE>
					<LEFT_SINGLE_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#_Push)</LEFT_SINGLE_CODE>
					<WWISE_EVENT>#WWISE_EVENT_SMALL_KNOB#</WWISE_EVENT>
					<WWISE_EVENT_1>#WWISE_EVENT_PUSH_1#</WWISE_EVENT_1>
					<WWISE_EVENT_2>#WWISE_EVENT_PUSH_2#</WWISE_EVENT_2>
					<THRESHOLD_MOUSE>0.01</THRESHOLD_MOUSE>
					<THRESHOLD_INPUT>0.0005</THRESHOLD_INPUT>
				</UseTemplate>
			</Component>

			<Condition NotEmpty="NODE_ID_MIDDLE_KNOB_BACKLIGHT">
				<Component ID="#NODE_ID_MIDDLE_KNOB_BACKLIGHT#" Node="#NODE_ID_MIDDLE_KNOB_BACKLIGHT#">
					<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
						<EMISSIVE_CODE>#KNOB_EMISSIVE_CODE#</EMISSIVE_CODE>
					</UseTemplate>
				</Component>
			</Condition>

			<UseTemplate Name="WT_G3000_GTC_Joystick_Template">
				<NODE_ID>#NODE_ID_LEFT_JOYSTICK#</NODE_ID>
				<PAN_NODE_ID>#PAN_NODE_ID_LEFT_JOYSTICK#</PAN_NODE_ID>
				<ANIM_NAME_L_R>#ANIM_NAME_LEFT_JOYSTICK_L_R#</ANIM_NAME_L_R>
				<ANIM_NAME_DN_UP>#ANIM_NAME_LEFT_JOYSTICK_DN_UP#</ANIM_NAME_DN_UP>
				<ANIM_NAME_KNOB>#ANIM_NAME_LEFT_JOYSTICK_KNOB#</ANIM_NAME_KNOB>
				<ANIM_NAME_PUSH>#ANIM_NAME_LEFT_JOYSTICK_PUSH#</ANIM_NAME_PUSH>

				<Condition Valid="LEFT_JOYSTICK_INVERT_ANIM_L_R">
					<INVERT_ANIM_L_R>True</INVERT_ANIM_L_R>
				</Condition>
				<Condition Valid="LEFT_JOYSTICK_INVERT_ANIM_DN_UP">
					<INVERT_ANIM_DN_UP>True</INVERT_ANIM_DN_UP>
				</Condition>

				<THRESHOLD_X_INPUT>#JOYSTICK_THRESHOLD_X_INPUT#</THRESHOLD_X_INPUT>
				<THRESHOLD_X_MOUSE>#JOYSTICK_THRESHOLD_X_MOUSE#</THRESHOLD_X_MOUSE>
				<THRESHOLD_Y_INPUT>#JOYSTICK_THRESHOLD_Y_INPUT#</THRESHOLD_Y_INPUT>
				<THRESHOLD_Y_MOUSE>#JOYSTICK_THRESHOLD_Y_MOUSE#</THRESHOLD_Y_MOUSE>
			</UseTemplate>

			<Condition NotEmpty="NODE_ID_LEFT_JOYSTICK_BACKLIGHT">
				<Component ID="#NODE_ID_LEFT_JOYSTICK_BACKLIGHT#" Node="#NODE_ID_LEFT_JOYSTICK_BACKLIGHT#">
					<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
						<EMISSIVE_CODE>#KNOB_EMISSIVE_CODE#</EMISSIVE_CODE>
					</UseTemplate>
				</Component>
			</Condition>
		</Component>
	</Template>

	<Template Name="WT_G3000_GTC_Joystick_Template">
		<Parameters Type="Default">
			<NODE_ID>AS580_KNOB_02#SUFFIX_ID#</NODE_ID>
			<PAN_NODE_ID>AS580_Joystick_Range_Pan#SUFFIX_ID#</PAN_NODE_ID>
			<ANIM_NAME_L_R>AS580_Joystick_Range_L_R#SUFFIX_ID#</ANIM_NAME_L_R>
			<ANIM_NAME_DN_UP>AS580_Joystick_Range_DN_UP#SUFFIX_ID#</ANIM_NAME_DN_UP>
			<ANIM_NAME_KNOB>AS580_Knob_02#SUFFIX_ID#</ANIM_NAME_KNOB>
			<ANIM_NAME_PUSH>AS580_Joystick_Range_Push#SUFFIX_ID#</ANIM_NAME_PUSH>

			<THRESHOLD_X_INPUT>0.1</THRESHOLD_X_INPUT>
			<THRESHOLD_X_MOUSE>0.01</THRESHOLD_X_MOUSE>
			<THRESHOLD_Y_INPUT>0.1</THRESHOLD_Y_INPUT>
			<THRESHOLD_Y_MOUSE>0.01</THRESHOLD_Y_MOUSE>
		</Parameters>
		<Parameters Type="Override">
			<KNOB_X_ID>Joystick_Range_Pan_X</KNOB_X_ID>
			<KNOB_Y_ID>Joystick_Range_Pan_Y</KNOB_Y_ID>
			<KNOB_ZOOM_ID>Joystick_Range_Zoom</KNOB_ZOOM_ID>
			<BTN_ID_CURSOR>Joystick_Range_Cursor</BTN_ID_CURSOR>
		</Parameters>
		<Parameters Type="Override">
			<JOYSTICK_X_CODE_LEFT>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_X_ID#_Left)</JOYSTICK_X_CODE_LEFT>
			<JOYSTICK_X_CODE_RIGHT>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_X_ID#_Right)</JOYSTICK_X_CODE_RIGHT>
			<JOYSTICK_Y_CODE_DOWN>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_Y_ID#_Down)</JOYSTICK_Y_CODE_DOWN>
			<JOYSTICK_Y_CODE_UP>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_Y_ID#_Up)</JOYSTICK_Y_CODE_UP>
			<WHEEL_UP_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ZOOM_ID#_Out)</WHEEL_UP_CODE>
			<WHEEL_DOWN_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ZOOM_ID#_In)</WHEEL_DOWN_CODE>
			<LEFT_SINGLE_CODE_DEFAULT_IM>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_CURSOR#_Push) </LEFT_SINGLE_CODE_DEFAULT_IM>
		</Parameters>

		<Component ID="#NODE_ID#" Node="#NODE_ID#">
			<Parameters Type="Override">
				<TOOLTIP_TITLE>@TT_Package.AS570_JOYSTICK_TITLE</TOOLTIP_TITLE>
				<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#KNOB_X_ID#</TOOLTIP_ENTRY_0>
				<TOOLTIP_ENTRY_1>#INPUT_EVENT_ID_SOURCE#_#KNOB_Y_ID#</TOOLTIP_ENTRY_1>
				<TOOLTIP_ENTRY_2>#INPUT_EVENT_ID_SOURCE#_#KNOB_ZOOM_ID#</TOOLTIP_ENTRY_2>
				<TOOLTIP_ENTRY_3>#INPUT_EVENT_ID_SOURCE#_#BTN_ID_CURSOR#</TOOLTIP_ENTRY_3>
			</Parameters>
			<UseInputEvent ID="AS3000">
				<KNOB_ID>#KNOB_X_ID#</KNOB_ID>
				<TT_DESCRIPTION_ID>@TT_Package.AS570_JOYSTICK_ACTION</TT_DESCRIPTION_ID>
				<TT_ICON>MOVE_AXIS_X</TT_ICON>
				<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:#KEY_PREFIX#_Joystick_Right) } els{ (&gt;H:#KEY_PREFIX#_Joystick_Left) }</SET_STATE_EXTERNAL>
				<BINDING_INC_0>Right</BINDING_INC_0>
				<BINDING_INC_0_PARAM_0>1</BINDING_INC_0_PARAM_0>
				<BINDING_DEC_0>Left</BINDING_DEC_0>
				<BINDING_DEC_0_PARAM_0>1</BINDING_DEC_0_PARAM_0>
				<NO_ANIM_VAR_UPDATE>True</NO_ANIM_VAR_UPDATE>
			</UseInputEvent>
			<UseInputEvent ID="AS3000">
				<KNOB_ID>#KNOB_Y_ID#</KNOB_ID>
				<TT_DESCRIPTION_ID>@TT_Package.AS570_JOYSTICK_POINTER_ACTION</TT_DESCRIPTION_ID>
				<TT_ICON>MOVE_AXIS_Y</TT_ICON>
				<TT_INTERACTION>PRIMARY_DOWN+Y_AXIS</TT_INTERACTION>
				<TT_INTERACTION_LOCKABLE>Y_AXIS</TT_INTERACTION_LOCKABLE>
				<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:#KEY_PREFIX#_Joystick_Up) } els{ (&gt;H:#KEY_PREFIX#_Joystick_Down) }</SET_STATE_EXTERNAL>
				<BINDING_INC_0>Up</BINDING_INC_0>
				<BINDING_INC_0_PARAM_0>1</BINDING_INC_0_PARAM_0>
				<BINDING_DEC_0>Down</BINDING_DEC_0>
				<BINDING_DEC_0_PARAM_0>1</BINDING_DEC_0_PARAM_0>
				<NO_ANIM_VAR_UPDATE>True</NO_ANIM_VAR_UPDATE>
			</UseInputEvent>
			<UseInputEvent ID="AS3000">
				<KNOB_ID>#KNOB_ZOOM_ID#</KNOB_ID>
				<TT_DESCRIPTION_ID>@TT_Package.AS1000_PFD_JOYSTICK_RANGE_KNOB_ACTION</TT_DESCRIPTION_ID>
				<TT_INTERACTION>PRIMARY_DOWN+INCREASE+DECREASE</TT_INTERACTION>
				<TT_INTERACTION_LOCKABLE>INCREASE+DECREASE</TT_INTERACTION_LOCKABLE>
				<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:#KEY_PREFIX#_Joystick_INC) } els{ (&gt;H:#KEY_PREFIX#_Joystick_DEC) }</SET_STATE_EXTERNAL>
				<BINDING_INC_0>Out</BINDING_INC_0>
				<BINDING_INC_0_PARAM_0>1</BINDING_INC_0_PARAM_0>
				<BINDING_DEC_0>In</BINDING_DEC_0>
				<BINDING_DEC_0_PARAM_0>1</BINDING_DEC_0_PARAM_0>
			</UseInputEvent>
			<UseTemplate Name="ASOBO_AS1000_Push_Event_Only_Template">
				<BTN_ID>#BTN_ID_CURSOR#</BTN_ID>
				<TT_DESCRIPTION_ID>@TT_Package.AS580_LOWER_KNOB_ACTION_PUSH</TT_DESCRIPTION_ID>
				<SET_STATE_EXTERNAL>(&gt;H:#KEY_PREFIX#_Joystick_Push)</SET_STATE_EXTERNAL>
				<!--
				<TT_VALUE>(L:WTG3000_#FUNCTION#_MapShowCursor:#FUNCTION_ID#) if{ @TT_On } els{ @TT_Off }</TT_VALUE>
				<TT_VALUE_IS_DYNAMIC>True</TT_VALUE_IS_DYNAMIC>
				-->
			</UseTemplate>
			<UseTemplate Name="ASOBO_GT_Joystick_Interaction_Knob">
				<!-- Spacial case for DRAG_IM to condense inputs to only one node -->
				<ID>#INPUT_EVENT_ID_SOURCE#_Joystick</ID>
				<UPDATE_FREQUENCY>20</UPDATE_FREQUENCY>
				<ADDITIONAL_EVENT_HANDLING>(M:Event) 'LeftSingle' 'RightSingle' (M:InputType) 1 == ? scmi 0 == if{ (&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_CURSOR#_Push) quit }</ADDITIONAL_EVENT_HANDLING>
				<MOUSEFLAGS_DRAG_IM>RightSingle+LeftAll+Wheel+Lock+Unlock</MOUSEFLAGS_DRAG_IM>
				<COUNT>18</COUNT>
				<WWISE_EVENT_1>as3000_knob_push_button_on</WWISE_EVENT_1>
				<WWISE_EVENT_2>as3000_knob_push_button_off</WWISE_EVENT_2>
				<WWISE_EVENT>as3000_knob_turn</WWISE_EVENT>
				<WWISE_JOYSTICK_EVENT_L>as3000_joystick</WWISE_JOYSTICK_EVENT_L>
				<WWISE_JOYSTICK_EVENT_R>as3000_joystick</WWISE_JOYSTICK_EVENT_R >
				<WWISE_JOYSTICK_EVENT_UP>as3000_joystick</WWISE_JOYSTICK_EVENT_UP>
				<WWISE_JOYSTICK_EVENT_DN>as3000_joystick</WWISE_JOYSTICK_EVENT_DN>
			</UseTemplate>
		</Component>

		<!-- Required for legacy interaction mode compat -->
		<Component ID="#PAN_NODE_ID#" Node="#PAN_NODE_ID#">
			<UseTemplate Name="ASOBO_GT_Joystick_Interaction_Pan">
				<WWISE_JOYSTICK_EVENT_L>as3000_joystick</WWISE_JOYSTICK_EVENT_L>
				<WWISE_JOYSTICK_EVENT_R>as3000_joystick</WWISE_JOYSTICK_EVENT_R >
				<WWISE_JOYSTICK_EVENT_UP>as3000_joystick</WWISE_JOYSTICK_EVENT_UP>
				<WWISE_JOYSTICK_EVENT_DN>as3000_joystick</WWISE_JOYSTICK_EVENT_DN>
			</UseTemplate>
		</Component>
	</Template>

	<!-- Horizontal GTC (580) -->

	<Template Name="WT_G3000_GTC_Horizontal_SubTemplate">
		<Parameters Type="Default">
			<AS3000>AS580</AS3000>
		</Parameters>

		<Parameters Type="Default">
			<FAILURE>(A:CIRCUIT AVIONICS ON, Bool)</FAILURE>
			<ANIM_NEVER_FAILS>True</ANIM_NEVER_FAILS>
			<INPUT_EVENT_ID_SOURCE>AS3000_TSC_#ID#</INPUT_EVENT_ID_SOURCE>
			<SCREEN_NODE_ID>AS3000_TSC_#ID#</SCREEN_NODE_ID>
		</Parameters>

		<Parameters Type="Override">
			<UseParametersFn Name="G3000_GTC_Backlight_Var">
			</UseParametersFn>
		</Parameters>

		<Parameters Type="Default">
			<EMISSIVE_MIN>0.02</EMISSIVE_MIN>
			<EMISSIVE_MAX>1</EMISSIVE_MAX>
			<EMISSIVE_GAMMA>2.2</EMISSIVE_GAMMA>
			<EMISSIVE_INPUT>(#BACKLIGHT_VAR_TYPE#:#BACKLIGHT_VAR_NAME#)</EMISSIVE_INPUT>
		</Parameters>

		<Parameters Type="Default">
			<SCREEN_EMISSIVE_MIN>#EMISSIVE_MIN#</SCREEN_EMISSIVE_MIN>
			<SCREEN_EMISSIVE_MAX>#EMISSIVE_MAX#</SCREEN_EMISSIVE_MAX>
			<SCREEN_EMISSIVE_GAMMA>#EMISSIVE_GAMMA#</SCREEN_EMISSIVE_GAMMA>
			<SCREEN_EMISSIVE_INPUT>#EMISSIVE_INPUT#</SCREEN_EMISSIVE_INPUT>
		</Parameters>

		<Parameters Type="Default">
			<KNOB_EMISSIVE_MIN>#EMISSIVE_MIN#</KNOB_EMISSIVE_MIN>
			<KNOB_EMISSIVE_MAX>#EMISSIVE_MAX#</KNOB_EMISSIVE_MAX>
			<KNOB_EMISSIVE_GAMMA>#EMISSIVE_GAMMA#</KNOB_EMISSIVE_GAMMA>
			<KNOB_EMISSIVE_INPUT>#EMISSIVE_INPUT#</KNOB_EMISSIVE_INPUT>
		</Parameters>

		<Parameters Type="Default">
			<KEY_EMISSIVE_MIN>#EMISSIVE_MIN#</KEY_EMISSIVE_MIN>
			<KEY_EMISSIVE_MAX>#EMISSIVE_MAX#</KEY_EMISSIVE_MAX>
			<KEY_EMISSIVE_GAMMA>#EMISSIVE_GAMMA#</KEY_EMISSIVE_GAMMA>
			<KEY_EMISSIVE_INPUT>#EMISSIVE_INPUT#</KEY_EMISSIVE_INPUT>
		</Parameters>

		<Parameters Type="Default">
			<UseParametersFn Name="G3000_Emissive_Power_Curve">
				<PREFIX>SCREEN</PREFIX>
			</UseParametersFn>
			<UseParametersFn Name="G3000_Emissive_Power_Curve">
				<PREFIX>KNOB</PREFIX>
			</UseParametersFn>
			<UseParametersFn Name="G3000_Emissive_Power_Curve">
				<PREFIX>KEY</PREFIX>
			</UseParametersFn>
		</Parameters>

		<Parameters Type="Override">
			<TOOLTIP_TITLE_SOFTKEY_1>@TT_Package.AS580_PUSH_SOFTKEY_TITLE_PFD</TOOLTIP_TITLE_SOFTKEY_1>
			<TT_DESCRIPTION_ID_SOFTKEY_1>@TT_Package.AS580_PUSH_SOFTKEY_ACTION_PFD</TT_DESCRIPTION_ID_SOFTKEY_1>
			<TOOLTIP_TITLE_SOFTKEY_2>@TT_Package.AS580_PUSH_SOFTKEY_TITLE_MFD</TOOLTIP_TITLE_SOFTKEY_2>
			<TT_DESCRIPTION_ID_SOFTKEY_2>@TT_Package.AS580_PUSH_SOFTKEY_ACTION_MFD</TT_DESCRIPTION_ID_SOFTKEY_2>
			<TOOLTIP_TITLE_SOFTKEY_3>@TT_Package.AS580_PUSH_SOFTKEY_TITLE_NAV_COM</TOOLTIP_TITLE_SOFTKEY_3>
			<TT_DESCRIPTION_ID_SOFTKEY_3>@TT_Package.AS580_PUSH_SOFTKEY_ACTION_NAV_COM</TT_DESCRIPTION_ID_SOFTKEY_3>
		</Parameters>

		<UseTemplate Name="ASOBO_AS3000_TSC_Screen_Template">
			<NODE_ID>#SCREEN_NODE_ID#</NODE_ID>
			<AS3000>AS3000</AS3000>
			<USE_STATIC_SCREEN_EMISSIVE>True</USE_STATIC_SCREEN_EMISSIVE>
			<EMISSIVE_CODE>#SCREEN_EMISSIVE_CODE#</EMISSIVE_CODE>
		</UseTemplate>

		<Loop>
			<Setup>
				<Param>KEY_ID</Param>
				<From>1</From>
				<Inc>1</Inc>
				<To>3</To>
			</Setup>
			<Do>
				<UseTemplate Name="ASOBO_AS3000_Softkey_Template">
					<NODE_ID>AS580_Push_0#KEY_ID#_#DISPLAY_POS##SUFFIX_ID#</NODE_ID>
					<ANIM_NAME>AS580_Push_0#KEY_ID#_#DISPLAY_POS##SUFFIX_ID#</ANIM_NAME>
					<PART_ID>AS580_Push_0#KEY_ID#_#DISPLAY_POS#</PART_ID>
					<HTML_KEY>AS3000_TSC_Horizontal_#ID#_SoftKey_#KEY_ID#</HTML_KEY>
					<BTN_ID>Softkey_#KEY_ID#</BTN_ID>
					<EMISSIVE_CODE>#KEY_EMISSIVE_CODE#</EMISSIVE_CODE>
					<TOOLTIP_TITLE_SOFTKEY Process="Param">TOOLTIP_TITLE_SOFTKEY_#KEY_ID#</TOOLTIP_TITLE_SOFTKEY>
					<TT_DESCRIPTION_ID_SOFTKEY Process="Param">TT_DESCRIPTION_ID_SOFTKEY_#KEY_ID#</TT_DESCRIPTION_ID_SOFTKEY>
					<TT_DESCRIPTION_INDEX/>
				</UseTemplate>
			</Do>
		</Loop>
		<Component ID="AS580_Knobs_#ID#">
			<Parameters Type="Default">
				<WWISE_EVENT_LARGE_KNOB>AS580_big_knob_turn</WWISE_EVENT_LARGE_KNOB>
				<WWISE_EVENT_SMALL_KNOB>AS580_knob_turn</WWISE_EVENT_SMALL_KNOB>
				<WWISE_EVENT_PUSH_1>AS580_knob_push_button_on</WWISE_EVENT_PUSH_1>
				<WWISE_EVENT_PUSH_2>AS580_knob_push_button_off</WWISE_EVENT_PUSH_2>
				<COUNT>36</COUNT>
				<NODE_ID_LARGE_KNOB>AS580_KNOB_01_#DISPLAY_POS#</NODE_ID_LARGE_KNOB>
				<ANIM_NAME_LARGE_KNOB>AS580_KNOB_01_#DISPLAY_POS#</ANIM_NAME_LARGE_KNOB>
				<NODE_ID_SMALL_KNOB>AS580_KNOB_01_SMALL_#DISPLAY_POS#</NODE_ID_SMALL_KNOB>
				<ANIM_NAME_SMALL_KNOB>AS580_KNOB_01_SMALL_#DISPLAY_POS#</ANIM_NAME_SMALL_KNOB>
				<ANIM_NAME_SMALL_KNOB_PUSH>AS580_Push_KNOB_01_SMALL_#DISPLAY_POS#</ANIM_NAME_SMALL_KNOB_PUSH>
				<NODE_ID_LOWER_KNOB>AS580_KNOB_02_#DISPLAY_POS#</NODE_ID_LOWER_KNOB>
				<ANIM_NAME_LOWER_KNOB>AS580_KNOB_02_#DISPLAY_POS#</ANIM_NAME_LOWER_KNOB>
				<ANIM_NAME_LOWER_KNOB_PUSH>AS580_Push_KNOB_02_#DISPLAY_POS#</ANIM_NAME_LOWER_KNOB_PUSH>
				<LONG_CLICK_TIME>2</LONG_CLICK_TIME>

				<Condition Valid="NO_KNOB_BACKLIGHT">
					<False>
						<NODE_ID_LARGE_KNOB_BACKLIGHT>AS580_KNOB_01_BACKLIGHT_#DISPLAY_POS#</NODE_ID_LARGE_KNOB_BACKLIGHT>
						<NODE_ID_LOWER_KNOB_BACKLIGHT>AS580_KNOB_02_BACKLIGHT_#DISPLAY_POS#</NODE_ID_LOWER_KNOB_BACKLIGHT>
					</False>
				</Condition>
			</Parameters>

			<Component ID="#NODE_ID_LARGE_KNOB#" Node="#NODE_ID_LARGE_KNOB#">
				<Parameters Type="Override">
					<KNOB_ID>Frequency_Knob_Mhz</KNOB_ID>
				</Parameters>
				<Parameters Type="Override">
					<TOOLTIP_TITLE>@TT_Package.AS580_UPPER_KNOB_OUTER_TITLE</TOOLTIP_TITLE>
					<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</TOOLTIP_ENTRY_0>
				</Parameters>
				<UseInputEvent ID="AS3000">
					<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:AS3000_TSC_Horizontal_#ID#_TopKnob_Large_INC) } els{ (&gt;H:AS3000_TSC_Horizontal_#ID#_TopKnob_Large_DEC) }</SET_STATE_EXTERNAL>
					<TT_DESCRIPTION_ID>@TT_Package.AS580_UPPER_KNOB_ACTION</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseTemplate Name="ASOBO_GT_Knob_Infinite">
					<ANIM_NAME>#ANIM_NAME_LARGE_KNOB#</ANIM_NAME>
					<PART_ID>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</PART_ID>
					<CLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Inc)</CLOCKWISE_CODE>
					<ANTICLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Dec)</ANTICLOCKWISE_CODE>
					<WWISE_EVENT>#WWISE_EVENT_LARGE_KNOB#</WWISE_EVENT>
					<THRESHOLD_INPUT>0.0005</THRESHOLD_INPUT>
					<THRESHOLD_MOUSE>0.005</THRESHOLD_MOUSE>
				</UseTemplate>
			</Component>

			<Condition NotEmpty="NODE_ID_LARGE_KNOB_BACKLIGHT">
				<Component ID="#NODE_ID_LARGE_KNOB_BACKLIGHT#" Node="#NODE_ID_LARGE_KNOB_BACKLIGHT#">
					<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
						<EMISSIVE_CODE>#KNOB_EMISSIVE_CODE#</EMISSIVE_CODE>
					</UseTemplate>
				</Component>
			</Condition>

			<Component ID="#NODE_ID_SMALL_KNOB#" Node="#NODE_ID_SMALL_KNOB#">
				<Parameters Type="Override">
					<KNOB_ID>Frequency_Knob_Khz</KNOB_ID>
					<BTN_ID_EXTERNAL>Frequency_Swap</BTN_ID_EXTERNAL>
				</Parameters>
				<Parameters Type="Override">
					<TOOLTIP_TITLE>@TT_Package.AS580_UPPER_KNOB_INNER_TITLE</TOOLTIP_TITLE>
					<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</TOOLTIP_ENTRY_0>
					<TOOLTIP_ENTRY_1>#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#</TOOLTIP_ENTRY_1>
				</Parameters>
				<UseInputEvent ID="AS3000">
					<TT_INTERACTION>PRIMARY_DOWN+SECONDARY_DOWN</TT_INTERACTION>
					<TT_INTERACTION_LOCKABLE>PRIMARY_DOWN</TT_INTERACTION_LOCKABLE>
					<BTN_ID>#BTN_ID_EXTERNAL#</BTN_ID>
					<!-- Configure event processing -->
					<SET_ARG_COUNT>2</SET_ARG_COUNT><!-- p0: Press/Release, p1:PressDuration, 0 = infinite (if pressing) -->
					<SET_STATE_EXTERNAL>p0 sp0
						l0 0 == if{ (O:_PressTime) 0 &gt; if{ (E:SIMULATION TIME, second) (O:_PressTime) - #LONG_CLICK_TIME# &lt; if{ (&gt;H:AS3000_TSC_Horizontal_#ID#_TopKnob_Push) 0 (&gt;O:_PressTime) } els{ (&gt;H:AS3000_TSC_Horizontal_#ID#_TopKnob_Push_Long) 0 (&gt;O:_PressTime) } } }
						l0 1 == if{ (E:SIMULATION TIME, second) (&gt;O:_PressTime) p1 (&gt;O:_PressForDuration) }
					</SET_STATE_EXTERNAL>
					<ANIM>EXTERNAL</ANIM>
					<BINDING_INC_0>Push</BINDING_INC_0>
					<BINDING_INC_0_PARAM_0>1</BINDING_INC_0_PARAM_0>
					<BINDING_DEC_0>Release</BINDING_DEC_0>
					<BINDING_DEC_0_PARAM_0>0</BINDING_DEC_0_PARAM_0>
					<!-- Configure binding for longpress event -->
					<BINDING_SET_0>Push_Long</BINDING_SET_0>
					<BINDING_SET_0_PARAM_0>1</BINDING_SET_0_PARAM_0>
					<BINDING_SET_0_PARAM_1>#LONG_CLICK_TIME#</BINDING_SET_0_PARAM_1>
					<TT_DESCRIPTION_ID>@TT_Package.AS580_UPPER_KNOB_INNER_ACTION_PUSH</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseInputEvent ID="AS3000">
					<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:AS3000_TSC_Horizontal_#ID#_TopKnob_Small_INC) } els{ (&gt;H:AS3000_TSC_Horizontal_#ID#_TopKnob_Small_DEC) }</SET_STATE_EXTERNAL>
					<TT_DESCRIPTION_ID>@TT_Package.AS580_UPPER_KNOB_ACTION</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseTemplate Name="ASOBO_GT_Knob_Infinite_Push_Timed">
					<CURSOR_LEFT>TurnLeftSmall</CURSOR_LEFT>
					<CURSOR_RIGHT>TurnRightSmall</CURSOR_RIGHT>
					<ANIM_NAME_KNOB>#ANIM_NAME_SMALL_KNOB#</ANIM_NAME_KNOB>
					<ANIM_NAME_PUSH>#ANIM_NAME_SMALL_KNOB_PUSH#</ANIM_NAME_PUSH>
					<PART_ID>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</PART_ID>
					<CLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Inc)</CLOCKWISE_CODE>
					<ANTICLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Dec)</ANTICLOCKWISE_CODE>
					<LEFT_SINGLE_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#_Push)</LEFT_SINGLE_CODE>
					<LEFT_LEAVE_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#_Release)</LEFT_LEAVE_CODE>
					<TIMER_EVENTS_HANDLED_EXTERNALLY>True</TIMER_EVENTS_HANDLED_EXTERNALLY>
					<LOCKABLE_EVENT_SECONDARY>LeftRelease</LOCKABLE_EVENT_SECONDARY>
					<NO_LOCK_EVENT_SECONDARY>RightRelease</NO_LOCK_EVENT_SECONDARY>
					<MOUSEFLAGS_DRAG_IM>RightAll+LeftAll+Wheel+Lock+Unlock</MOUSEFLAGS_DRAG_IM>
					<WWISE_EVENT>#WWISE_EVENT_SMALL_KNOB#</WWISE_EVENT>
					<WWISE_EVENT_1>#WWISE_EVENT_PUSH_1#</WWISE_EVENT_1>
					<WWISE_EVENT_2>#WWISE_EVENT_PUSH_2#</WWISE_EVENT_2>
					<THRESHOLD_X_INPUT>0.05</THRESHOLD_X_INPUT>
					<THRESHOLD_X_MOUSE>0.001</THRESHOLD_X_MOUSE>
				</UseTemplate>
			</Component>

			<Component ID="#NODE_ID_LOWER_KNOB#" Node="#NODE_ID_LOWER_KNOB#">
				<Parameters Type="Override">
					<KNOB_ID>Multifunction_Knob</KNOB_ID>
					<BTN_ID_EXTERNAL>Multifunction_Knob_Button</BTN_ID_EXTERNAL>
				</Parameters>
				<Parameters Type="Override">
					<TOOLTIP_TITLE>@TT_Package.AS580_LOWER_KNOB_TITLE</TOOLTIP_TITLE>
					<TOOLTIP_ENTRY_0>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</TOOLTIP_ENTRY_0>
					<TOOLTIP_ENTRY_1>#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#</TOOLTIP_ENTRY_1>
				</Parameters>
				<UseInputEvent ID="AS3000">
					<BTN_ID>#BTN_ID_EXTERNAL#</BTN_ID>
					<ANIM>HOLD</ANIM>
					<ANIM_VAR>_PushAnimVar</ANIM_VAR>
					<TT_INTERACTION>PRIMARY_DOWN+SECONDARY_DOWN</TT_INTERACTION>
					<TT_INTERACTION_LOCKABLE>PRIMARY_DOWN</TT_INTERACTION_LOCKABLE>
					<BINDING_INC_0>Push</BINDING_INC_0>
					<BINDING_INC_0_PARAM_0>1</BINDING_INC_0_PARAM_0>
					<SET_STATE_EXTERNAL>p0 if{ (&gt;H:AS3000_TSC_Horizontal_#ID#_BottomKnob_Push) }</SET_STATE_EXTERNAL>
					<TT_DESCRIPTION_ID>@TT_Package.AS580_LOWER_KNOB_ACTION_PUSH</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseInputEvent ID="AS3000">
					<SET_STATE_EXTERNAL>p0 0 &gt; if{ (&gt;H:AS3000_TSC_Horizontal_#ID#_BottomKnob_Small_INC) } els{ (&gt;H:AS3000_TSC_Horizontal_#ID#_BottomKnob_Small_DEC) }</SET_STATE_EXTERNAL>
					<TT_DESCRIPTION_ID>@TT_Package.AS580_LOWER_KNOB_ACTION</TT_DESCRIPTION_ID>
				</UseInputEvent>
				<UseTemplate Name="ASOBO_GT_Knob_Infinite_Push">
					<ANIM_NAME_KNOB>#ANIM_NAME_LOWER_KNOB#</ANIM_NAME_KNOB>
					<ANIM_NAME_PUSH>#ANIM_NAME_LOWER_KNOB_PUSH#</ANIM_NAME_PUSH>
					<PART_ID>#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#</PART_ID>
					<CLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Inc)</CLOCKWISE_CODE>
					<ANTICLOCKWISE_CODE>1 (&gt;B:#INPUT_EVENT_ID_SOURCE#_#KNOB_ID#_Dec)</ANTICLOCKWISE_CODE>
					<LEFT_SINGLE_CODE>(&gt;B:#INPUT_EVENT_ID_SOURCE#_#BTN_ID_EXTERNAL#_Push)</LEFT_SINGLE_CODE>
					<WWISE_EVENT>#WWISE_EVENT_SMALL_KNOB#</WWISE_EVENT>
					<WWISE_EVENT_1>#WWISE_EVENT_PUSH_1#</WWISE_EVENT_1>
					<WWISE_EVENT_2>#WWISE_EVENT_PUSH_2#</WWISE_EVENT_2>
				</UseTemplate>
			</Component>

			<Condition NotEmpty="NODE_ID_LOWER_KNOB_BACKLIGHT">
				<Component ID="#NODE_ID_LOWER_KNOB_BACKLIGHT#" Node="#NODE_ID_LOWER_KNOB_BACKLIGHT#">
					<UseTemplate Name="ASOBO_GT_Emissive_Gauge">
						<EMISSIVE_CODE>#KNOB_EMISSIVE_CODE#</EMISSIVE_CODE>
					</UseTemplate>
				</Component>
			</Condition>
		</Component>
	</Template>
</ModelBehaviors>